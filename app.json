{
  "expo": {
    "name": "UG",
    "slug": "urban-gravity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ug-logo.png",
    "scheme": "ug",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/onboarding/Onboarding-1.png",
        "backgroundImage": "./assets/images/onboarding/Onboarding-1.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/ug-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ug-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/HubotSans-Regular.ttf",
            "./assets/fonts/HubotSans-Medium.ttf",
            "./assets/fonts/HubotSans-SemiBold.ttf",
            "./assets/fonts/HubotSans-Bold.ttf",
            "./assets/fonts/HubotSans-ExtraBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fda62561-c299-4702-979a-36388fc57502"
      }
    },
    "owner": "dharvo"
  }
}
